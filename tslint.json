{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [true, "single"],
    "member-access": [true, "no-public"],
    "variable-name": {
      "options": [
        "check-format",
        "allow-leading-underscore"
      ]
    }
  },
  "rulesDirectory": [],
  "exclude": [
    "node_modules"
  ]
}